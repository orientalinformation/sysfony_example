{% extends 'base.html.twig' %}

{% block body %}

<body class="hold-transition skin-blue sidebar-mini">
	<div>
		<!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <section>
                <div class="row">
                    <div class="col-lg-12">
                        <ul class="nav nav-tabs">
                            <li class=""><a href="/settings/mesh" aria-expanded="false"> Mesh</a></li>
                            <li class=""><a href="/settings/calculation" aria-expanded="false">Calculation</a></li>
                            <li class="active"><a href="/settings/result" aria-expanded="false">Result</a></li>
                        </ul>
                    </div>
                </div>
            </section>

            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>Result<small>Summary info</small></h1>
                <ol class="breadcrumb">
                    <li><a href="/"><i class="fa fa-dashboard"></i> Home</a></li>
                    <li class="active">Result</li>
                </ol>
            </section>
            {% for message in app.flashes('success') %}
                <div class="alert alert-success alert-dismissible btn-alert-success">
                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">Ã—</button>
                    <h4><i class="icon fa fa-check"></i> Alert!</h4>
                    <span style="color:#ffffff";><strong>{{ message }}</strong></span>
                </div>
            {% endfor %}

            <!-- Main content -->
            <section class="content">

                <!-- Main row -->
                <div class="row">
                    <div class="nav-tabs-custom">
                        <div class="tab-content">
                            <div class="row">
                                <div class="col-md-6">

                                </div>
                                <div class="col-md-6 ">
                                    <div class="row">
                                        <div class="col-md-11">
                                            <a href="#" class="pull-right" data-toggle="modal" data-target="#helpProduction" id="popoverData" data-placement="bottom" data-original-title="Help" data-trigger="manual">
                                                <i class="fa fa-question-circle " style="font-size: 34px; padding: 0 13px;"></i>
                                            </a>
                                        </div>
                                        <div class="col-md-1"></div>
                                    </div>
                                </div>
                            </div>
                            
                            <!-- /.box-header -->
                            <div class="box-body">
                                <form name="formResultSetting" id="formResultSetting" action="/settings/save" method="POST">
                                    <div class="box-body">
                                        <div class="form-group">
                                            <h3>Coordinates of the points of follow-up</h3>
                                        </div>
                                        <div class="table-responsive">
                                            <table class="table table-bordered">
                                                <tbody>
                                                  <tr>
                                                    <th colspan="3">Top %</th>
                                                    <th colspan="3">Inside %</th>
                                                    <th colspan="3">Bottom %</th>
                                                  </tr>
                                                  <tr>
                                                    <td>
                                                        <div class="row">
                                                            <div class="col-md-3">1</div>
                                                            <div class="col-md-9">
                                                                <input type="text" name="_axis1Top" id="_axis1Top" class="form-control" placeholder="0.00" size="5" maxlength="20" value="{{ axis1TopSurf }}">
                                                            </div>
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <div class="row">
                                                            <div class="col-md-3">2</div>
                                                            <div class="col-md-9">
                                                                <input type="text" name="_axis2Top" id="_axis2Top" class="form-control" placeholder="0.00" size="5" maxlength="20" value="{{ axis2TopSurf }}">
                                                            </div>
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <div class="row">
                                                            <div class="col-md-3">3</div>
                                                            <div class="col-md-9">
                                                                <input type="text" name="_axis3Top" id="_axis3Top" class="form-control" placeholder="0.00" size="5" maxlength="20" value="{{ axis3TopSurf }}">
                                                            </div>
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <div class="row">
                                                            <div class="col-md-3">1</div>
                                                            <div class="col-md-9">
                                                                <input type="text" name="_axis1Int" id="_axis1Int" class="form-control" placeholder="0.00" size="5" maxlength="20" value="{{ axis1IntPt }}">
                                                            </div>
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <div class="row">
                                                            <div class="col-md-3">2</div>
                                                            <div class="col-md-9">
                                                                <input type="text" name="_axis2Int" id="_axis2Int" class="form-control" placeholder="0.00" size="5" maxlength="20" value="{{ axis2IntPt }}">
                                                            </div>
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <div class="row">
                                                            <div class="col-md-3">3</div>
                                                            <div class="col-md-9">
                                                                <input type="text" name="_axis3Int" id="_axis3Int" class="form-control" placeholder="0.00" size="5" maxlength="20" value="{{ axis3IntPt }}" >
                                                            </div>
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <div class="row">
                                                            <div class="col-md-3">1</div>
                                                            <div class="col-md-9">
                                                                <input type="text" name="_axis1Bot" id="_axis1Bot" class="form-control" placeholder="0.00" size="5" maxlength="20" value="{{ axis1BotSurf }}">
                                                            </div>
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <div class="row">
                                                            <div class="col-md-3">2</div>
                                                            <div class="col-md-9">
                                                                <input type="text" name="_axis2Bot" id="_axis2Bot" class="form-control" placeholder="0.00" size="5" maxlength="20" value="{{ axis2BotSurf }}">
                                                            </div>
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <div class="row">
                                                            <div class="col-md-3">3</div>
                                                            <div class="col-md-9">
                                                                <input type="text" name="_axis3Bot" id="_axis3Bot" class="form-control" placeholder="0.00" size="5" maxlength="20" value="{{ axis3BotSurf }}">
                                                            </div>
                                                        </div>
                                                    </td>
                                                  </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                        <br/>
                                        <div class="form-group">
                                            <h3>Coordinates of the plan of follow-up</h3>
                                        </div>
                                        <div class="table-responsive">
                                            <table class="table table-bordered">
                                                <tbody>
                                                  <tr>
                                                    <th >Plan 12 %</th>
                                                    <th >Plan 13 %</th>
                                                    <th >Plan 23 %</th>
                                                  </tr>
                                                  <tr>
                                                    <td>
                                                        <div class="row">
                                                            <div class="col-md-3">2</div>
                                                            <div class="col-md-9">
                                                                <input type="text" name="_plan1Value" id="_plan1Value" class="form-control" placeholder="0.00" size="8" maxlength="20" value="{{ axis1PL23 }}">
                                                            </div>
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <div class="row">
                                                            <div class="col-md-3">3</div>
                                                            <div class="col-md-9">
                                                                <input type="text" name="_plan2Value" id="_plan2Value" class="form-control" placeholder="0.00" size="8" maxlength="20" value="{{ axis2PL13 }}">
                                                            </div>
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <div class="row">
                                                            <div class="col-md-3">1</div>
                                                            <div class="col-md-9">
                                                                <input type="text" name="_plan3Value" id="_plan3Value" class="form-control" placeholder="0.00" size="8" maxlength="20"  value="{{ axis3PL12 }}">
                                                            </div>
                                                        </div>
                                                    </td>
                                                  </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                        <br/>
                                        <div class="form-group">
                                            <h3>Coordinates of the axis of follow-up</h3>
                                        </div>
                                        <div class="table-responsive">
                                            <table class="table table-bordered">
                                                <tbody>
                                                  <tr>
                                                    <th colspan="2">Axis 1 %</th>
                                                    <th colspan="2">Axis 2 %</th>
                                                    <th colspan="2">Axis 3 %</th>
                                                  </tr>
                                                  <tr>
                                                    <td>
                                                        <div class="row">
                                                            <div class="col-md-3">2</div>
                                                            <div class="col-md-9">
                                                                <input type="text" name="_axis2Axe1" id="_axis2Axe1" class="form-control" placeholder="0.00" size="8" maxlength="20" value="{{ axis2Axe1 }}">
                                                            </div>
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <div class="row">
                                                            <div class="col-md-3">3</div>
                                                            <div class="col-md-9">
                                                                <input type="text" name="_axis3Axe1" id="_axis3Axe1" class="form-control" placeholder="0.00" size="8" maxlength="20" value="{{ axis3Axe1 }}">
                                                            </div>
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <div class="row">
                                                            <div class="col-md-3">1</div>
                                                            <div class="col-md-9">
                                                                <input type="text" name="_axis1Axe2" id="_axis1Axe2" class="form-control" placeholder="0.00" size="8" maxlength="20"  value="{{ axis1Axe2 }}">
                                                            </div>
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <div class="row">
                                                            <div class="col-md-3">3</div>
                                                            <div class="col-md-9">
                                                                <input type="text" name="_axis3Axe2" id="_axis3Axe2" class="form-control" placeholder="0.00" size="8" maxlength="20" value="{{ axis3Axe2 }}">
                                                            </div>
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <div class="row">
                                                            <div class="col-md-3">1</div>
                                                            <div class="col-md-9">
                                                                <input type="text" name="_axis1Axe3" id="_axis1Axe3" class="form-control" placeholder="0.00" size="8" maxlength="20" value="{{ axis1Axe3 }}">
                                                            </div>
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <div class="row">
                                                            <div class="col-md-3">2</div>
                                                            <div class="col-md-9">
                                                                <input type="text" name="_axis2Axe3" id="_axis2Axe3" class="form-control" placeholder="0.00" size="8" maxlength="20" value="{{ axis2Axe3 }}">
                                                            </div>
                                                        </div>
                                                    </td>
                                                
                                                  </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                        <br/>
                                        <div class="form-group button">
                                            <button type="button" name="btnResultSave" id="btnResultSave" class="btn btn-primary btn-flat buttons">Save</button>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>

                </div>
                <!-- /.row (main row) -->
                <!-- click help -->
                <div class="modal fade" id="helpProduction">
                    <div class="modal-dialog modal-lg">
                        <div class="modal-content">
                            <div class="modal-header">
                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                    <span aria-hidden="true">&times;</span></button>
                                <h4 class="modal-title">Setting Page</h4>
                            </div>
                            <div class="modal-body">
                                Help Result Setting
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-danger btn-flat button-block" data-dismiss="modal">Close</button>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- ./help -->
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        {{ include('includes/footer.html.twig') }}
	</div>
</body>

{% endblock %}