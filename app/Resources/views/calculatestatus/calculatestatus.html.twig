{% extends 'base.html.twig' %}

{% block body %}

<body class="hold-transition skin-blue sidebar-mini">
	<div class="content-wrapper">
        <section>
            <div class="row">
                <div class="col-lg-12">
                    <ul class="nav nav-tabs">
                        <li class="active"><a href="#tab_1" data-toggle="tab" aria-expanded="false">{{'Calculation Status'|trans}}</a></li>
                    </ul>
                </div>
            </div>
        </section>

        <!-- Content Header (Page header) -->
        <section class="content-header">
            <h1>
        {{'Sample Page'|trans}}
        <small>{{'Label of page'|trans}}</small>
      </h1>
            <ol class="breadcrumb">
                <li><a href="#"><i class="fa fa-dashboard"></i> {{'Home'|trans}}</a></li>
                <li class="active">{{'Sample page'|trans}}</li>
            </ol>
        </section>

        <!-- Main content -->
        <section class="content">

            <!-- Main row -->
            <div class="row">
                <div class="nav-tabs-custom">
                    <div class="tab-content">
                        <div class="tab-pane active" id="tab_1">
                            <table style="width:100%">
                                <tr>
                                    <th>{{'Study name'|trans}}</th>
                                    <th>{{'Study status'|trans}}</th>
                                    <th>{{'Stop'|trans}}</th>
                                </tr>
                                <tr>
                                    <td>{{'Jill'|trans}}</td>
                                    <td>{{'Smith'|trans}}</td>
                                    <td><a href="#"><i class="fa fa-stop-circle-o" aria-hidden="true"></i></a></td>
                                </tr>
                                <tr>
                                    <td>{{'Eve'|trans}}</td>
                                    <td>{{'Jackson'|trans}}</td>
                                    <td><a href="#"><i class="fa fa-stop-circle-o" aria-hidden="true"></i></a></td>
                                </tr>
                                <tr>
                                    <td>{{'John'|trans}}</td>
                                    <td>{{'Doe'|trans}}</td>
                                    <td><a href="#"><i class="fa fa-stop-circle-o" aria-hidden="true"></i></a></td>
                                </tr>
                            </table>
                        </div>
                    </div>
                </div>

            </div>
            <!-- /.row (main row) -->
            <!-- click preferance user -->
            <div class="modal fade" id="preferUser">
                <div class="modal-dialog modal-lg">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span></button>
                            <h4 class="modal-title">{{'User Preferences'|trans}}</h4>
                        </div>
                        <div class="modal-body">
                            <div class="row">
                                <div class="col-md-5">
                                    <label>{{'Password'|trans}}</label>
                                    <input type="text" name="oldPass" placeholder="Old Password" class="form-control">
                                    <br/>
                                    <input type="text" name="newPass" placeholder="New Password" class="form-control">
                                    <br/>
                                    <input type="text" name="conPass" placeholder="Confirm Password" class="form-control">
                                </div>
                                <div class="col-md-6"> {{'Language and monetary unit'|trans}}
                                    <div class="form-group">
                                        <label>{{'Language'|trans}}</label>
                                        <select data-placeholder="--Menu--" class="form-control chosen-select">
                                            <option></option>
                                            <option>MenuItem1</option>
                                            <option>MenuItem2</option>
                                        </select>
                                    </div>
                                    <div class="form-group">
                                        <label>{{'Monetary'|trans}}</label>
                                        <select data-placeholder="--Menu--" class="form-control chosen-select">
                                            <option></option>
                                            <option>MenuItem1</option>
                                            <option>MenuItem2</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group Gbutton">
                                    <button class="btn btn-flat btn-danger buttons margin" data-dismiss="modal"><i class="fa fa-times"></i> {{'Cancel'|trans}}</button>
                                    <button class="btn btn-flat btn-success buttons"><i class="fa fa-check"></i> {{'Confirm'|trans}}</button>
                                </div>
                            </div>

                            <h4>{{'Units'|trans}}</h4>

                            <div class="table-responsive">
                                <div class="row">
                                    <div class="col-md-6" style="background: #eee">
                                        <h5 style="text-align: center;"><strong>{{'Type'|trans}}</strong></h5>
                                    </div>
                                    <div class="col-md-6" style="background: #eee">
                                        <h5 style="text-align: center;"><strong>{{'Symbol'|trans}}</strong></h5>
                                    </div>
                                </div>
                                <div style="width:100%; height:200px; overflow:auto;">
                                    <table class="table table-bordered">
                                        <tr>
                                            <td colspan="2">Conductivity</td>
                                            <td colspan="2">
                                                <select data-placeholder="--Menu--" class="form-control chosen-select">
                                                    <option></option>
                                                    <option>MenuItem1</option>
                                                    <option>MenuItem2</option>
                                                </select>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td colspan="2">Consumption unit</td>
                                            <td colspan="2">
                                                <select data-placeholder="--Menu--" class="form-control chosen-select">
                                                    <option></option>
                                                    <option>MenuItem1</option>
                                                    <option>MenuItem2</option>
                                                </select>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td colspan="2">Consumption unit(CO2)</td>
                                            <td colspan="2">
                                                <select data-placeholder="--Menu--" class="form-control chosen-select">
                                                    <option></option>
                                                    <option>MenuItem1</option>
                                                    <option>MenuItem2</option>
                                                </select>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td colspan="2">Consumption unit(LN2)</td>
                                            <td colspan="2">
                                                <select data-placeholder="--Menu--" class="form-control chosen-select">
                                                    <option></option>
                                                    <option>MenuItem1</option>
                                                    <option>MenuItem2</option>
                                                </select>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td colspan="2">Heat Losses per hour</td>
                                            <td colspan="2">
                                                <select data-placeholder="--Menu--" class="form-control chosen-select">
                                                    <option></option>
                                                    <option>MenuItem1</option>
                                                    <option>MenuItem2</option>
                                                </select>
                                            </td>
                                        </tr>

                                    </table>
                                </div>
                                <div class="row">
                                    <div class="form-group Gbutton">
                                        <button class="btn btn-flat btn-danger buttons margin" data-dismiss="modal"><i class="fa fa-times"></i> {{'Cancel'|trans}}</button>
                                        <button class="btn btn-flat btn-success buttons"><i class="fa fa-check"></i> {{'Confirm'|trans}}</button>
                                    </div>
                                </div>
                            </div>
                            <h4>{{'Default selection for equipment page'|trans}}</h4>
                            <div class="form-group">

                                <div class="row">
                                    <div class="col-md-4">
                                        {{'Type of Refrigeration'|trans}}
                                    </div>
                                    <div class="col-md-8">
                                        <select data-placeholder="--Menu--" class="form-control chosen-select">
                                            <option></option>
                                            <option>MenuItem1</option>
                                            <option>MenuItem2</option>
                                        </select>

                                    </div>
                                </div>
                                <br/>
                                <div class="row">
                                    <div class="col-md-4">
                                        {{'Manufacturer'|trans}}
                                    </div>
                                    <div class="col-md-8">
                                        <select data-placeholder="--Menu--" class="form-control chosen-select">
                                            <option></option>
                                            <option>MenuItem1</option>
                                            <option>MenuItem2</option>
                                        </select>

                                    </div>
                                </div>
                                <br/>
                                <div class="row">
                                    <div class="col-md-4">
                                        {{'Equipment Series'|trans}}
                                    </div>
                                    <div class="col-md-8">
                                        <select data-placeholder="--Menu--" class="form-control chosen-select">
                                            <option></option>
                                            <option>MenuItem1</option>
                                            <option>MenuItem2</option>
                                        </select>

                                    </div>
                                </div>
                                <br/>
                                <div class="row">
                                    <div class="col-md-4">
                                        {{'Equipment origin'|trans}}
                                    </div>
                                    <div class="col-md-8">
                                        <select data-placeholder="--Menu--" class="form-control chosen-select">
                                            <option></option>
                                            <option>MenuItem1</option>
                                            <option>MenuItem2</option>
                                        </select>

                                    </div>
                                </div>
                                <br/>
                                <div class="row">
                                    <div class="col-md-4">
                                        {{'Process type'|trans}}
                                    </div>
                                    <div class="col-md-8">
                                        <select data-placeholder="--Menu--" class="form-control chosen-select">
                                            <option></option>
                                            <option>MenuItem1</option>
                                            <option>MenuItem2</option>
                                        </select>

                                    </div>
                                </div>
                                <br/>
                                <div class="row">
                                    <div class="col-md-4">
                                        {{'Model'|trans}}
                                    </div>
                                    <div class="col-md-8">
                                        <select data-placeholder="--Menu--" class="form-control chosen-select">
                                            <option></option>
                                            <option>MenuItem1</option>
                                            <option>MenuItem2</option>
                                        </select>

                                    </div>
                                </div>
                                <div class="form-group Gbutton">
                                    <button class="btn btn-flat btn-danger buttons margin" data-dismiss="modal"><i class="fa fa-times"></i> {{'Cancel'|trans}}</button>
                                    <button class="btn btn-flat btn-success buttons"><i class="fa fa-check"></i> {{'Confirm'|trans}}</button>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
            <!-- /. preferance user -->
        </section>
        <!-- /.content -->
    </div>
{% include 'includes/footer.html.twig' %}
</body>

{% endblock %}
