<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <section>
                <div class="row">
                    <div class="col-lg-12">
                        <ul class="nav nav-tabs">

                            <li class=""><a href="/home/fromlogin" aria-expanded="false">{{ 'Dashboard' | trans}}</a></li>
                            <li class=""><a href="/definition" aria-expanded="false">{{ 'Definitition' | trans }}</a></li>
                            <li class=""><a href="/production" aria-expanded="false">{{ 'Production' | trans }}</a></li>
                            <li class=""><a href="/productChar" aria-expanded="false">{{ 'Product and characteristic' | trans }}</a></li>
                            <li class=""><a href="/mesh" aria-expanded="false">{{ 'Mesh initial temperature' | trans }}</a></li>
                            <li class=""><a href="/packing" aria-expanded="false">{{ 'Packing' | trans }}</a></li>
                            <li class="active"><a href="/equipments" aria-expanded="false">{{ 'Equipment' | trans }}</a></li>
                            <li class=""><a href="/line" aria-expanded="false">{{ 'Line' | trans }}</a></li>
                        </ul>
                    </div>
                </div>
            </section>

            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>
        {{ 'Equipment' | trans}}
        <small>{{ 'Summary info' | trans }}</small>
      </h1>
                <ol class="breadcrumb">
                    <li><a href="/home"><i class="fa fa-dashboard"></i>{{ 'Home' | trans }}</a></li>
                    <li class="active">{{ 'Equipment' | trans }}</li>
                </ol>
            </section>

            <!-- Main content -->
            <section class="content">
                <span class="label label-danger">{{ 'Study Name' | trans }} :  {{ studyName }}</span>
                <!-- Main row -->
                <div class="row">
                    <div class="nav-tabs-custom">
                        <div class="tab-content">
                            <div class="row">
                                <div class="col-md-6">
                                </div>
                                <div class="col-md-6 ">
                                    <div class="row">
                                        <div class="col-md-11">

                                        </div>
                                        <div class="col-md-1">
                                            <a href="#" class="pull-right" data-toggle="modal" data-target="#helpEquipment" id="popoverData" data-placement="bottom" data-original-title="Help" data-trigger="manual">
                                                <i class="fa fa-question-circle " style="font-size: 34px; padding: 0 13px;"></i>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                           <!--  {% for flash_massage in app.session.flashbag.get('Noitce') %}
                                <span style="color:red";><strong>{{flash_massage}}</strong></span>
                            {% endfor %} -->

                            {% for flash_massage in app.session.flashbag.get('WARNING') %}
                            <div class="alert alert-warning alert-dismissible">
                                <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                                <h4><i class="fa fa-exclamation-triangle" aria-hidden="true"></i> {{ 'Warning' | trans }} !</h4>
                                <span style="color:#ffffff" ;><strong>{{flash_massage}}</strong></span>
                            </div>
                            {% endfor %}
                            <div class="row">
                                <form id="idform-addEquipment" action="/addEquipment" method="post"> 
                                    <div class="col-md-12">
                                        <h4>{{ 'Selection criteria' | trans }}</h4>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <div class="styled-select">
                                                    <label class="" for="Type of Refrigeration">{{ 'Type of Refrigeration' | trans }} : </label>
                                                    <select id="id-refrigeration" data-placeholder="-- All --" class="chosen-select" name="name-refrigeration">
                                                        <option value="-1"></option>
                                                        <option value="-1">-- All --</option>
                                                        {% for item in ret.listRefrigeration %}
                                                            {% if ret.valueDefault.userEnergy == item.idCoolingFamily %}
                                                            <option selected value="{{ item.idCoolingFamily }}">{{ item.label }}</option>
                                                            {% else %}
                                                            <option value="{{ item.idCoolingFamily }}">{{ item.label }}</option>
                                                            {% endif %}
                                                        {% endfor %}
                                                    </select>
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <div class="styled-select">
                                                    <label class="" for="Manufacturer">{{ 'Manufacturer' | trans }} : </label>
                                                    <select id="id-manufacturer" data-placeholder="-- All --" class="chosen-select base-refri" name="name-manufacturer">
                                                        <option value="-1"></option>
                                                        <option selected value="-1">-- All --</option>
                                                         {% for item in ret.listManufacturer %}
                                                            <option value="{{ item.constructor }}">{{ item.constructor }}</option>
                                                        {% endfor %}
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <div class="styled-select">
                                                    <label class="" for="Equipment Series">{{ 'Equipment Series' | trans }}: </label>
                                                    <select id="id-equipseries" data-placeholder="-- All --" class="chosen-select base-refri" name="name-equipseries">
                                                        <option value="-1"></option>
                                                        <option value="-1">-- All --</option>
                                                        {% for item in ret.listEquipSeries %}
                                                        {% if item.idFamily == ret.valueDefault.userFamily %}
                                                            <option selected value="{{ item.idFamily }}">{{ item.label }}</option>
                                                        {% else %}
                                                            <option value="{{ item.idFamily }}">{{ item.label }}</option>
                                                            {% endif %}
                                                        {% endfor %}
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <div class="styled-select">
                                                    <label class="" for="Equipment origin">{{ 'Equipment origin' | trans }}: </label>
                                                    <select id="id-equiporigin" data-placeholder="-- All --" class="chosen-select base-refri" name="name-equiporigin">
                                                        <option value="-1"></option>
                                                        <option value="-1">-- All --</option>
                                                        {% for item in ret.listEquipOrigin %}
                                                         {% if item.std == ret.valueDefault.userOrigine %}
                                                            <option selected value="{{ item.std }}">{{ item.label }}</option>
                                                        {% else %}
                                                            <option value="{{ item.std }}">{{ item.label }}</option>
                                                            {% endif %}
                                                        {% endfor %}
                                                    </select>
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <div class="styled-select">
                                                    <label class="" for="Process type">{{ 'Process type' | trans }} : </label>
                                                    <select id="id-processtype" data-placeholder="-- All --" class="chosen-select base-refri" name="name-processtype">
                                                        <option value="-1"></option>
                                                        <option value="-1">-- All --</option>
                                                        {% for item in ret.listProcessType %}
                                                        {% if item.batchProcess == ret.valueDefault.userProcess %}
                                                            <option selected value="{{ item.batchProcess }}">{{ item.label }}</option>
                                                        {% else %}
                                                            <option value="{{ item.batchProcess }}">{{ item.label }}</option>
                                                            {% endif %}
                                                        {% endfor %}
                                                    </select>
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <div class="styled-select">
                                                    <label class="" for="Model">{{ 'Model' | trans }} : </label>
                                                    <select id="id-model" data-placeholder="-- All --" class="chosen-select base-refri" name="name-model">
                                                        <option value="-1"></option>
                                                        <option value="-1">-- All --</option>
                                                        {% for item in ret.listModel %}
                                                        {% if item.idEquipseries == ret.valueDefault.userModel %}
                                                            <option selected value="{{ item.idEquipseries }}">{{ item.label }}</option>
                                                        {% else %}
                                                            <option value="{{ item.idEquipseries }}">{{ item.label }}</option>
                                                            {% endif %}
                                                        {% endfor %}
                                                    </select>
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <div class="styled-select">
                                                    <label class="" for="Size">{{ 'Size' | trans }} : </label>
                                                    <select id="id-size" data-placeholder="-- All --" class="chosen-select base-refri" name="name-size">
                                                        <option value="-1"></option>
                                                        <option selected value="-1">-- All --</option>
                                                        {% for item in ret.listSize %}
                                                            <option value="{{ item.eqpLength }}x{{ item.eqpWidth }}">{{ item.eqpLength }} x {{ item.eqpWidth }}</option>
                                                        {% endfor %}
                                                    </select>
                                                </div>

                                            </div>

                                            {% if ret.listDisplay.optionEco %}
                                                <div>
                                                    <h4 style="margin-left: -16px">{{ 'Price of the Cryogen' | trans }}</h4>
                                                    <div class="form-group">
                                                        <label class="">{{ 'Define the price of Cryogen' | trans }}</label>
                                                        <div class="input-group">
                                                            <input class="form-control input-radiusProD" id="input-price-cryogen" type="text" placeholder="Input price" name="price-of-cryogen" value="{{ ret.energyPrice }}">
                                                            <span class="input-group-addon" id="basic-addon2">&ensp;EURO&ensp;</span>
                                                        </div>
                                                    </div>
                                                </div>
                                        
                                            {% endif %}
                                        </div>
                                        <!-- div class="col-md-6" style="padding: 0 15px;" -->
                                        <div class="col-md-6">
                                            <form class="form-horizontalinline">
                                                <h4><p class="h4-margin">{{ 'Search Results' | trans }}</p></h4>
                                                <div class="table-responsive">
                                                    <select id="id-equipment" class="form-control" size="13" style="width: 100%" name="name-equipment">
                                                        <p  style=" padding: 10px; width: 345px; height: 276px;">
                                                            {% for item in ret.listEquipment %}
                                                                {% if item.checkModalESize is null %}
                                                                    <option class="{{ item.classOption }}" value="{{ item.idEquip }}">{{ item.equipName }}
                                                                    </option>
                                                                {% else %}
                                                                    <option class="pos-equipment-modal {{ item.classOption }}" value="{{ item.idEquip }}">{{ item.equipName }}
                                                                    </option>
                                                                {% endif %}
                                                            {% endfor %}
                                                        </p>
                                                    </select>
                                                </div>
                                                <!-- div class="form-group" -->
                                            </form>
                                            {% if ret.listDisplay.loadingRate != 1 %}
                                            <h4> <p class="">{{ 'Loading Rate' | trans }}</p></h4>
                                            <label class="">{{ 'Spaces (Length and Width)' | trans }}</label>
                                            <div class="row">
                                                <div class="col-md-3">
                                                    <div class="form-group">
                                                        <div class="input-group">
                                                            <input class="form-control input-radiusProD" placeholder="0" id="input-equip-lenght" name="input-equip-lenght" type="text" value="{{ ret.loadingRateFrame.length }}">
                                                                <span class="input-group-addon" id="basic-addon2">cm</span>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-3">
                                                    <div class="form-group">
                                                        <div class="input-group">
                                                            <input class="form-control input-radiusProD" placeholder="0" id="input-equip-width" name="input-equip-width" type="text" value="{{ ret.loadingRateFrame.width }}">
                                                                <span class="input-group-addon" id="basic-addon2">cm</span>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            {% endif %}
                                            <!-- form class="form-horizontal inline" -->
                                        </div>
                                        <!-- div class="col-md-6" -->
                                    </div>
                                    <!-- div class="col-md-12" -->
                                </form>
                            </div>
                            <!-- div class="row" -->
                            <div id="first">
                                <h4>{{ 'Selected Equipment' | trans }}</h4>
                                <form action="/recalculateEquipment" id="form-select-equipment" method="post" class="form-horizontal inline">
                                    <div class="form-group gButton" style="margin-left: -10px;">
                                        <button class="btn bg-orange margin btn-flat" type="submit"><i class="fa fa-calculator"></i> {{ 'Recalculate equipment parameter' | trans }}</button>
                                        {% if ret.listDisplay.unselect == 2 %}
                                        <button class="btn bg-orange margin btn-flat" id="btn-unseletect-all" type="button"><i class="fa fa-refresh"></i> {{ 'Unseletect All' | trans }}</button>
                                        {% endif %}
                                    </div>
                                </form>
                                <div class="table-responsive">
                                    <table class="table table-bordered">
                                        <tr>
                                            <th>No</th>
                                            {% if ret.listDisplay.unselect == 2 %}
                                            <th>{{ 'Calculate' | trans }}</th>
                                            {% endif %}
                                            <th>{{ 'Name' | trans }}</th>
                                            <th>{{ 'Product orientation' | trans }}</th>
                                            <th>{{ 'Residence' | trans }}/{{ 'dwelling time' | trans }} (s)</th>
                                            <th>{{ 'Control' | trans }}</br> {{ 'temperature' | trans }} (C)</th>
                                            <th>{{ 'Convection' | trans }}</br> {{ 'setting' | trans }} (Hz)</th>
                                            <th>{{ 'Conveyor coverage or' | trans }} </br>{{ 'quantity of product' | trans }}</br> {{ 'per batch' | trans }}</th>
                                            <th>{{ 'Delete' | trans }}</th>
                                        </tr>
                                        {% if ret.listStudyEquip != null %}
                                        {% set no=0 %}
                                        {% for item in ret.listStudyEquip %}
                                        {% set no = no+1%}
                                        <tr id="{{item.idStudyEquipments}}" data-nameequipment="{{ item.studyEquipName }}">
                                            <td><a href="#" class="id-no-modal" data-id="{{ item.idStudyEquipments }}" >{{no}}</a></td>
                                            {% if ret.listDisplay.unselect == 2 %}
                                            <td>
                                                <div class="checkbox icheck" >
                                                    <label>
                                                    {% if item.brainType == 0 %}
                                                        {% if (item.equipStatus ==0 or item.equipStatus==1) and (item.runCalculate == 1)  %}
                                                            <input type="checkbox" class="input-checked-economic" checked="checked" name="option1" class="first" value="{{ item.idStudyEquipments }}"> {{ 'Economic' | trans }}
                                                        
                                                        {% else %}
                                                            <input type="checkbox" class="input-checked-economic" name="option1" value="{{ item.idStudyEquipments }}" class="first"> {{ 'Economic' | trans }}
                                                        {% endif %}
                                                    {% else %}
                                                        <input type="checkbox" class="input-checked-economic" name="option1" value="{{ item.idStudyEquipments }}" class="first"> {{ 'Economic' | trans }}
                                                    {% endif %}
                                                    </label>
                                                </div>
                                            </td>
                                            {% endif %}
                                            <td>
                                            {% if item.checkModalName == 1 %}
                                                <a href="#" class="id-study-equip-name" data-idequip="{{ item.idStudyEquipments }}" >{{ item.studyEquipName }}</a>
                                            {% else %}
                                                <p style="color: #00c0ef">{{ item.studyEquipName }}</p>
                                            {% endif %}
                                            </td>
                                            <td>
                                                {% if item.checkModalOrientation == 1 %}
                                                     <a href="#" class="id-study-equip-orientation" data-idequip="{{ item.idStudyEquipments }}" >{{ item.orientation }}</a>
                                                {% else %}
                                                    <p style="color: #00c0ef">{{ item.orientation }}</p>
                                                {% endif %}
                                            </td>                                            
                                            <td>{{ item.tr | raw}}</td>
                                            <td>{{ item.ts | raw}}</td>
                                            <td>{{ item.vc | raw}}</td>
                                            <td><a href="#" class="id-study-equip-conveyor">{{ item.conveyor | raw}} </a>
                                            </td>
                                            <td>
                                                <a href="/delStudyEquip/{{item.idStudyEquipments}}" style="height: 30px; width: 40px; padding: 5px;" class="btn btn-block btn-danger btn-flat btn-lg" type="button" ><i class="w3-xxxlarge glyphicon glyphicon-remove"></i></a>
                                            </td>
                                        </tr>
                                        {% endfor %}
                                        {% endif %}
                                        
                                    </table>
                                </div>













                                
