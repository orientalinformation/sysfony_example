<div class="modal" id="preferUser">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" onclick="javascript:window.location.reload()" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">User Preferences</h4>
            </div>
            <div class="modal-body">
                <iframe frameborder="0" style="width:100%;height: 700px;"></iframe>
            </div>
        </div>
    </div>
</div>

