<header class="main-header">
    <!-- Logo -->
    <a href="/" class="logo">
        <!-- mini logo for sidebar mini 50x50 pixels -->
        <span class="logo-mini"><b>C</b>ST</span>
        <!-- logo for regular state and mobile devices -->
        <span class="logo-lg"><b>CryoSoft</b></span>
    </a>
    <!-- Header Navbar: style can be found in header.less -->
    <nav class="navbar navbar-static-top">
        <!-- Sidebar toggle button-->
        <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
            <span class="sr-only">Toggle navigation</span>
        </a>

        <div class="navbar-custom-menu">
            <ul class="nav navbar-nav">
                <!-- User Account: style can be found in dropdown.less -->
                <li class="dropdown user user-menu">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                        <img src="{{ asset('assets/dist/img/Icon/user.svg') }}" class="user-image" alt="User Image">

                        <span class="hidden-xs">{% if app.user %}
                                                    {{ app.user.usernam }}
                                                {% endif %}</span>
                    </a>
                    <ul class="dropdown-menu">
                        <!-- User image -->
                        <li class="user-header">
                            <img src="{{ asset('assets/dist/img/Icon/user.svg') }}" class="img-circle" alt="User Image">

                            <p>
                                {% if app.user %}
                                    {{ app.user.usernam }}
                                {% endif %} - AZ Corp
                                <small>Member since Nov. 2012</small>
                            </p>
                        </li>
                        <!-- Menu Footer-->
                        <li class="user-footer">
                            <div class="pull-right">
                                <a href="/logoutNow" class="btn btn-default  btn-flat"><i class="fa fa-sign-out"></i>{{'Sign out'|trans}}</a>
                            </div>
                            <div class="pull-left">
                                <a href="/profile" class="btn btn-default  btn-flat if-modal" data-toggle="modalx" data-target="#preferUser" >
                                    <i class="fa fa-users "></i> {{'Profile'|trans}}</a>
                            </div>
                        </li>
                    </ul>
                </li>
